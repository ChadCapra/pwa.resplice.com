{
  "name": "resplice-pwa-upgrade",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.4.5",
    "jwt-decode": "^2.2.0",
    "register-service-worker": "^1.0.0",
    "v-mask": "^1.3.2",
    "vue": "^2.5.17",
    "vue-awesome": "^3.1.0",
    "vue-croppie": "^1.3.12",
    "vue-moment": "^4.0.0",
    "vue-qrcode-reader": "^1.1.0",
    "vue-router": "^3.0.1",
    "vue-touch": "^2.0.0-beta.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
